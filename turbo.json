{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**"]
    },
    "//:lint": {},
    "//:lint:fix": {},
    "//:format": {},
    "//:format:fix": {},
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "generate": {
      "dependsOn": ["^generate"],
      "outputs": ["src/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "dependsOn": ["^test"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    }
  }
}
